    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/employee_nav_graph"
    app:startDestination="@id/signup_fragment">

    <fragment
        android:id="@+id/signup_fragment"
        android:name="com.example.employeelist.ui.employee.view.SignupFragment"
        tools:layout="@layout/signup_fragment">
        <action
            android:id="@+id/action_signup_to_employee_list"
